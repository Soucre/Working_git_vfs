<body main="" class="ng-scope" style="background-image: url( '/images/Login_Image.jpg');">
    <!-- Root element of template can't be <body>; see angular issue #1459 --><div id="signup">
        <div id="aside"><div class="imo-logo-wrapper"></div><h1 class="big-title ng-scope" translate="">Join with millions of people</h1><p class="subtitle ng-scope" translate="">Join millions of people using imo for free messaging, voice and video calls.</p></div><div id="login-register-container" register-login="">
            <div class="header"><a href="/Register" class="tab register" ng-click="selectTab(REGISTER)" ng-class="{selected: selectedForm == REGISTER}" translate="">Register</a><a href="/Login" class=" tab login selected" ng-click="selectTab(LOGIN)" ng-class="{selected: selectedForm == LOGIN}" translate="">login</a></div><!-- ngIf: selectedForm == LOGIN --><login ng-if="selectedForm == LOGIN" class="ng-scope">
                <form action="./Register   imo messenger_files/Register   imo messenger.html" method="post" id="login" validate="" novalidate="" class="ng-pristine ng-valid">
                    <div class="info ng-scope ng-hide" translate="" ng-show="params.jg">
                        Enter your imo Network account information. Then, sign into the app to
                        start chatting with the group!
                    </div><div ng-show="reason == &#39;uidpassword&#39;" class="global-error fancy-global-error ng-scope ng-hide" translate="">Invalid username or password.</div><div ng-show="reason == &#39;empty_country&#39;" class="global-error fancy-global-error ng-scope ng-hide" translate="">Please select your country</div><input type="hidden" name="phone_cc" value="VN"><validated-input class="phone fancy-input-container phone-field-with-country-code input-container ng-pristine ng-valid" placeholder="placeholder.phone"><label class="input-placeholder-wrapper"><input type="text" name="phone" class="fancy-input ng-pristine ng-valid" placeholder-attr="Mobile Phone Number" ng-model="phone_info.phone" ng-class="{error: showError(undefined.phone.$error)}" placeholder="Mobile Phone Number"></label><div class="error validated-input-error ng-binding ng-hide" ng-show="showError(undefined.phone.$error)"></div></validated-input><validated-input class="password fancy-input-container input-container ng-pristine ng-valid" placeholder="placeholder.password"><label class="input-placeholder-wrapper"><input type="password" name="password" class="fancy-input ng-pristine ng-valid" placeholder-attr="Password" ng-model="models.password" ng-class="{error: showError(undefined.password.$error)}" placeholder="Password"></label><div class="error validated-input-error ng-binding ng-hide" ng-show="showError(undefined.password.$error)"></div></validated-input><input type="hidden" name="trackId" value=""><input class="action-button fancy-button green big" type="submit" value="Login">
                </form>
            </login><!-- end ngIf: selectedForm == LOGIN --><!-- ngIf: selectedForm == REGISTER --><modals class="ng-scope"></modals>
